{
  "name": "japan-tourism-dashboard",
  "version": "2.0.0",
  "description": "Professional Japan Tourism Dashboard with JNTO data ingestion - International visitor arrivals with COVID-19 impact and recovery trends",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install && cd workers && npm install && cd ../client && npm install && npm run build",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "worker": "cd workers && node jntoDataIngestion.js",
    "test-worker": "cd workers && npm test",
    "heroku-postbuild": "npm run build",
    "postinstall": "cd workers && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "node-cron": "^3.0.3",
    "cheerio": "^1.0.0-rc.12",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "japan",
    "tourism",
    "dashboard",
    "react",
    "nodejs",
    "mongodb",
    "chartjs",
    "jnto",
    "data-ingestion",
    "covid-19",
    "analytics",
    "visualization"
  ],
  "author": "Japan Tourism Dashboard Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aravinditte/Japan-Tourism-Trends-Dashboard.git"
  },
  "homepage": "https://github.com/aravinditte/Japan-Tourism-Trends-Dashboard"
}